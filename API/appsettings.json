{
    "ConnectionStrings": {
        // Replace [MSSQLSERVERNAME] with the name of your SQL Server
        "DefaultConnection": "Server=[MSSQLSERVERNAME];Database=[DATABASENAME];Trusted_Connection=True;" // Use TrustServerCertificate=True; to enable SSL
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}
